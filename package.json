{
  "name": "react-webcomponent-example",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@polymer/lit-element": "^0.6.2",
    "@types/styled-components": "^4.0.3",
    "@webcomponents/webcomponentsjs": "^2.1.3",
    "chart.js": "^2.7.3",
    "globals": "^11.8.0",
    "lodash": "^4.17.11",
    "react": "^16.6.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.6.0",
    "react-scripts-ts": "3.1.0",
    "styled-components": "^4.0.3",
    "tslint-plugin-prettier": "^2.0.1"
  },
  "scripts": {
    "start": "cp -rf node_modules/@webcomponents/webcomponentsjs public/ && rm public/webcomponentsjs/package.json && react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "test:nowatch": "CI=true yarn test",
    "eject": "react-scripts-ts eject",
    "lint": "prettier --write 'src/**/*.{js,ts,jsx,tsx,json,css,md}'",
    "prebuild": "yarn test:nowatch",
    "prestart": "yarn lint",
    "pretest": "yarn lint",
    "commit": "yarn test && ./node_modules/.bin/git-cz",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@storybook/addon-actions": "^4.0.4",
    "@storybook/addon-info": "^4.0.4",
    "@storybook/addon-storyshots": "^4.0.4",
    "@storybook/react": "^4.0.4",
    "@types/fetch-mock": "^6.0.4",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-storyshots": "^3.4.3",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "commitizen": "^3.0.4",
    "fetch-mock": "^7.2.5",
    "husky": "^1.1.3",
    "jest-styled-components": "^6.2.2",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-test-renderer": "^16.6.1",
    "require-context": "^1.1.0",
    "semantic-release": "^15.10.7",
    "typescript": "^3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
